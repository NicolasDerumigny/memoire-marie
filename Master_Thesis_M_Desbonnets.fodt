      <text:p text:style-name="P63" loext:marker-style-name="T57"><text:span text:style-name="T57">2012</text:span><text:span text:style-name="T57"/></text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Table5.A1" office:value-type="string">
      <text:p text:style-name="P63" loext:marker-style-name="T57"><text:span text:style-name="T57">Ancien secrétaire du gouvernement </text:span><text:span text:style-name="T57"/></text:p>
     </table:table-cell>
